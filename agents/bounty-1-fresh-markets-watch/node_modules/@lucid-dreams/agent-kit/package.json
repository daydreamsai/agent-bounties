{
  "name": "@lucid-dreams/agent-kit",
  "version": "0.2.24",
  "description": "Build typed agent HTTP apps",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "default": "./dist/types.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "default": "./dist/utils.js"
    },
    "./utils/axllm": {
      "types": "./dist/utils/axllm.d.ts",
      "import": "./dist/utils/axllm.js",
      "default": "./dist/utils/axllm.js"
    },
    "./utils/x402": {
      "types": "./dist/utils/x402.d.ts",
      "import": "./dist/utils/x402.js",
      "default": "./dist/utils/x402.js"
    },
    "./erc8004": {
      "types": "./dist/erc8004.d.ts",
      "import": "./dist/erc8004.js",
      "default": "./dist/erc8004.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "AGENTS.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "prepare": "bun run build"
  },
  "dependencies": {
    "zod": "^4.1.12",
    "hono": "4.10.1",
    "x402-hono": "^0.7.1",
    "@lucid-dreams/agent-auth": "^0.2.24",
    "@lucid-dreams/client": "^0.2.24",
    "@lucid-dreams/agent-kit-identity": "^0.2.24",
    "x402-fetch": "^0.7.0",
    "x402": "^0.7.1",
    "viem": "^2.38.5",
    "@ax-llm/ax": "^14.0.37",
    "@ax-llm/ax-tools": "^14.0.37"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
